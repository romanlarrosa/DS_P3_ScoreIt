<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.scoreit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_socreit"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_scoreit_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".LocalActivity"></activity>
        <activity android:name=".LocalesActivity" />
        <activity android:name=".AddLocalActivity" />
        <activity android:name=".VerifyUsersActivity" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".Registro"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".WelcomeActivity"
            android:theme="@style/LoginTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>